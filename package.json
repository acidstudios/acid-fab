{
    "name": "acid-fab",
    "private": true,
    "version": "1.0.0",
    "description": "Material Floating Action Button implementation for Angular 4/5/6",
    "main": "./bundles/acid-fab.umd.js",
    "module": "./fesm5/acid-fab.js",
    "es2015": "./fesm2015/acid-fab.js",
    "esm5": "./esm5/acid-fab.js",
    "esm2015": "./esm2015/acid-fab.js",
    "fesm5": "./fesm5/acid-fab.js",
    "fesm2015": "./fesm2015/acid-fab.js",
    "typings": "./acid-fab.d.ts",
    "scripts": {
        "build": "node build.js",
        "build:demo:dev": "cross-env webpack --progress --color",
        "build:demo:prod": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js --progress --color",
        "test": "karma start",
        "test:watch": "karma start karma.conf.js --single-run false",
        "pack:lib": "npm run build && npm pack ./dist",
        "publish:lib": "npm run build && npm publish ./dist",
        "publish:lib:next": "npm run build && npm publish --tag next ./dist",
        "compodoc": "compodoc -p tsconfig.json",
        "compodoc:serve": "compodoc -s",
        "start": "cross-env webpack-dev-server --inline --progress --port 8080"
    },
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/acidstudios/acid-fab.git"
    },
    "bugs": {
        "url": "https://github.com/acidstudios/acid-fab/issues"
    },
    "homepage": "https://github.com/acidstudios/acid-fab",
    "keywords": [
        "angular",
        "javascript",
        "typescript"
    ],
    "license": "MIT",
    "dependencies": {
        "tslib": "^1.9.0"
    },
    "peerDependencies": {
        "@angular/common": ">= 6.0.0",
